<UserControl x:Class="CozyLauncher.Plugin.Guide.GuidePage.PageInfo.PageInfo2_1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CozyLauncher.Plugin.Guide.GuidePage.PageInfo"
             mc:Ignorable="d" 
             Width="800" Height="600">
    <UserControl.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Image3" UriSource="/CozyLauncher.Plugin.Guide;component/Resources/3.png"/>
            <BitmapImage x:Key="Image4" UriSource="/CozyLauncher.Plugin.Guide;component/Resources/4.png"/>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="114"></RowDefinition>
            <RowDefinition Height="159"></RowDefinition>
            <RowDefinition Height="200"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock FontSize="32" TextAlignment="Center" VerticalAlignment="Center" Grid.Row="0">计算器插件</TextBlock>
        <Image Source="{StaticResource Image3}" Grid.Row="1"></Image>
        <Image Source="{StaticResource Image4}" Grid.Row="2"></Image>
    </Grid>

</UserControl>
