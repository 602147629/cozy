<UserControl x:Class="CozyLauncher.Plugin.Guide.GuidePage.PageInfo.PageInfo2_2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CozyLauncher.Plugin.Guide.GuidePage.PageInfo"
             mc:Ignorable="d" 
             Width="800">
        <UserControl.Resources>
            <ResourceDictionary>
                <BitmapImage x:Key="Image5" UriSource="/CozyLauncher.Plugin.Guide;component/Resources/5.png"/>
                <BitmapImage x:Key="Image6" UriSource="/CozyLauncher.Plugin.Guide;component/Resources/6.png"/>
            </ResourceDictionary>
        </UserControl.Resources>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="110"></RowDefinition>
                <RowDefinition Height="205"></RowDefinition>
                <RowDefinition Height="302"></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock FontSize="32" TextAlignment="Center" VerticalAlignment="Center" Grid.Row="0">计算器插件</TextBlock>
            <Image Source="{StaticResource Image5}" Grid.Row="1"></Image>
            <Image Source="{StaticResource Image6}" Grid.Row="2"></Image>
        </Grid>
</UserControl>
