<Window x:Class="CozyLauncher.ConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CozyLauncher"
        xmlns:controls="clr-namespace:CozyLauncher.Controls"
        xmlns:viewmodel="clr-namespace:CozyLauncher.ViewModels"
        mc:Ignorable="d"
        Title="ConfigWindow" Height="300" Width="300">
    <Window.DataContext>
        <viewmodel:ConfigWindowViewModel x:Name="ViewModel"></viewmodel:ConfigWindowViewModel>
    </Window.DataContext>
    <StackPanel>
        <controls:HotkeyControl HotkeyText="{Binding HotkeyText, Mode=TwoWay}"></controls:HotkeyControl>
        <Button Content="submit" Command="{Binding SubmitCommand}"></Button>
        <ListView Height="235">
        </ListView>        
    </StackPanel>
</Window>
